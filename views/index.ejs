<%- include ('partials/header.ejs') %>

<body>
    <%- include ('partials/navbar.ejs') %>

    <div class="container">
        <hr>
        <div class="jumbotron">
            <h1 class="display-4">Bem-vindo</h1>
            
          </div>
        </br>
        <% articleFront.forEach(element => { %>
            <div class="card">
                <div class="card-header">
                    <h2><%= element.title %></h2>
                </div>
                <div class="card-body">
                    <a href="/<%= element.slug %>" class="btn btn-success">Ler artigo</a>
                </div>
            </div>
         
        <% }) %>

        <nav aria-label="Page navigation example" class="mt-3">
            <ul class="pagination">
              <li class="page-item"><a class="page-link" href="/articles/page/1">1</a></li>
              <li class="page-item"><a class="page-link" href="/articles/page/2 ">2</a></li>
              <li class="page-item"><a class="page-link" href="/articles/page/3">3</a></li>
              
            </ul>
          </nav>

    </div>
    
</body>

<%- include ('partials/footer.ejs') %>
